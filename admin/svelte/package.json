{
  "name": "ab-wp-bits-admin",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --config vite.menu-queries.config.ts && vite build --config vite.menu-conditions.config.ts",
    "build:main": "vite build",
    "build:menu-queries": "vite build --config vite.menu-queries.config.ts",
    "build:menu-conditions": "vite build --config vite.menu-conditions.config.ts",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.6",
    "@types/node": "^24.10.1",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  }
}
